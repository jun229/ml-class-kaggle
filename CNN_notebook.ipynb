   "source": [
    "# Cell 3 \n",
    "import torch, pickle, numpy as np\n",
    "from torch.utils.data import Dataset, DataLoader\n",
    "from torchvision import transforms\n",
    "\n",
    "# ---------- PREP ----------\n",
    "# X_raw, y already loaded by Cells 1‑2\n",
    "# Create one transform pipeline and keep it small (96×96 instead of 224×224)\n",
    "tf = transforms.Compose([\n",
    "    transforms.ToPILImage(),                 # 24×24 → PIL\n",
    "    transforms.Resize(96, antialias=True),   # 24×24 → 96×96\n",
    "    transforms.Grayscale(num_output_channels=3),  # 1‑ch → 3‑ch\n",
    "    transforms.ToTensor(),                   # (3,96,96) in [0,1]\n",
    "    transforms.Normalize(mean=[0.485,0.456,0.406],\n",
    "                         std =[0.229,0.224,0.225])\n",
    "])\n",
    "\n",
    "class RPSPair(Dataset):\n",
    "    def __init__(self, X_np, y_np=None, transform=None):\n",
    "        self.X = X_np.astype(np.uint8)\n",
    "        self.y = y_np\n",
    "        self.tf = transform\n",
    "    def __len__(self):  return len(self.X)\n",
    "    def __getitem__(self, idx):\n",
    "        img1, img2 = self.X[idx]\n",
    "        t1 = self.tf(img1)         # (3,96,96)\n",
    "        t2 = self.tf(img2)\n",
    "        x  = torch.cat([t1, t2], 0)   # (6,96,96)\n",
    "        if self.y is not None:\n",
    "            return x, (1 if self.y[idx] > 0 else 0)\n",
    "        return x\n",
    "\n",
    "batch_size = 8    # tiny batch keeps memory low\n",
    "train_ds   = RPSPair(X_raw, y, transform=tf)\n",
    "train_ld   = DataLoader(train_ds, batch_size=batch_size, shuffle=False, num_workers=0)\n",
    "print(\"One batch:\", next(iter(train_ld))[0].shape)  # (8,6,96,96)\n"
   ]
  },
