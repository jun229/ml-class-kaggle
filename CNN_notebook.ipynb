    "# CELL X – Manual CV for MLP + RBF‑SVM ensemble\n",
    "import numpy as np\n",
    "from sklearn.model_selection import StratifiedKFold\n",
    "from sklearn.svm import SVC\n",
    "from sklearn.pipeline import Pipeline\n",
    "from sklearn.decomposition import PCA\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "\n",
    "# Prepare data\n",
    "# X_raw: (N,2,24,24) numpy array\n",
    "# X_feat: (N,2560) numpy array of CNN features\n",
    "# y:    (N,) labels in {-1,+1}\n",
    "\n",
    "kf = StratifiedKFold(n_splits=3, shuffle=True, random_state=42)\n",
    "fold_scores = []\n",
    "\n",
    "for fold, (train_idx, val_idx) in enumerate(kf.split(X_raw, y), 1):\n",
    "    # Split raw and CNN feature sets\n",
    "    Xr_train, Xr_val = X_raw[train_idx], X_raw[val_idx]\n",
    "    Xc_train, Xc_val = X_feat[train_idx], X_feat[val_idx]\n",
    "    y_train,   y_val   = y[train_idx],   y[val_idx]\n",
    "    \n",
    "    # 1) Train RBF‑SVM pipeline on raw\n",
    "    svm = Pipeline([\n",
    "        (\"feat\",   RPSFeatureExtractor()),             # your extractor\n",
    "        (\"pca\",    PCA(n_components=100)),\n",
    "        (\"scaler\", StandardScaler()),\n",
    "        (\"cls\",    SVC(C=1, gamma=0.01))\n",
    "    ])\n",
    "    svm.fit(Xr_train, y_train)\n",
    "    svm_preds = svm.predict(Xr_val)                   # {-1,+1}\n",
    "\n",
    "    # 2) Train MLP on CNN features\n",
    "    mlp = MLPClassifier(\n",
    "        hidden_layer_sizes=(512,256),\n",
    "        activation=\"relu\",\n",
    "        alpha=1e-4,\n",
    "        max_iter=200,\n",
    "        random_state=42\n",
    "    )\n",
    "    # note: y_train in {-1,+1} → convert to {0,1} for MLP\n",
    "    mlp.fit(Xc_train, (y_train>0).astype(int))\n",
    "    mlp_preds = mlp.predict(Xc_val)                   # {0,1}\n",
    "    mlp_preds = np.where(mlp_preds>0, 1, -1)           # back to {-1,+1}\n",
    "\n",
    "    # 3) Ensemble by sum‑vote (tie → SVM)\n",
    "    s = svm_preds + mlp_preds\n",
    "    ens = np.where(s>0, 1, -1)\n",
    "    ties = (s==0)\n",
    "    ens[ties] = svm_preds[ties]\n",
    "\n",
    "    # 4) Accuracy\n",
    "    acc = np.mean(ens == y_val)\n",
    "    print(f\"Fold {fold} acc = {acc:.4f}\")\n",
    "    fold_scores.append(acc)\n",
    "\n",
    "print(f\"Ensemble 3‑fold CV accuracy = {np.mean(fold_scores):.4f} ± {np.std(fold_scores):.4f}\")\n"
