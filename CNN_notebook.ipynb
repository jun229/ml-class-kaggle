   "source": [
    "# Cell 4\n",
    "\n",
    "from torchvision import models\n",
    "device = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")\n",
    "\n",
    "# Use only MobileNet features (no classifier) and freeze params\n",
    "mobilenet = models.mobilenet_v2(weights=\"IMAGENET1K_V1\").features.to(device)\n",
    "mobilenet.eval()\n",
    "for p in mobilenet.parameters(): p.requires_grad = False\n",
    "\n",
    "def extract(loader):\n",
    "    feats, labs = [], []\n",
    "    with torch.inference_mode():\n",
    "        for xb, yb in loader:\n",
    "            xb, yb = xb.to(device), yb.numpy()\n",
    "            # split into two 3‑channel images\n",
    "            t1 = xb[:, 0:3, :, :]   # shape: (B,3,96,96)\n",
    "            t2 = xb[:, 3:6, :, :]   # shape: (B,3,96,96)\n",
    "\n",
    "            # extract features separately\n",
    "            f1 = mobilenet(t1)      # (B,1280,H',W')\n",
    "            f2 = mobilenet(t2)\n",
    "\n",
    "            # global average pool\n",
    "            f1 = f1.mean((2,3))     # (B,1280)\n",
    "            f2 = f2.mean((2,3))\n",
    "\n",
    "            # concatenate along feature dimension\n",
    "            f = torch.cat([f1, f2], dim=1)  # (B,2560)\n",
    "\n",
    "            feats.append(f.cpu().numpy())\n",
    "            labs.append(yb)\n",
    "    return np.vstack(feats), np.hstack(labs)\n",
    "\n",
    "\n",
    "X_feat, y_feat = extract(train_ld)\n",
    "print(\"Feature matrix:\", X_feat.shape)\n"
   ]
  },
